{
  "name": "antallen",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "sync": "browser-sync start --server --files \"**/*\"",
    "dev": "postcss ./static/css/input.css --output ./dist/output.css",
    "watch:dev": "nodemon -x npm run dev -w tailwind.config.js -w ./static/css/input.css -w ./dist/output.css",
    "watch:tailwind": "npx tailwindcss -i ./static/css/input.css -o ./dist/output.css --watch",
    "watch": "run-p watch:dev watch:tailwind sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecircleisround/antallen.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thecircleisround/antallen/issues"
  },
  "homepage": "https://github.com/thecircleisround/antallen#readme",
  "devDependencies": {
    "browser-sync": "^2.27.7",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^8.0.0",
    "gulp-postcss": "^9.0.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.18"
  }
}
